<h1>AttendanceEmployed</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Date</th>
            <td>{{ entity.date|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Clockin</th>
            <td>{{ entity.clockIn }}</td>
        </tr>
        <tr>
            <th>Clockout</th>
            <td>{{ entity.clockOut }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('attendanceemployed') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('attendanceemployed_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('attendanceemployed_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
