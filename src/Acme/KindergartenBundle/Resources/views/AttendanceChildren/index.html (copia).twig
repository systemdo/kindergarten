<h1>AttendanceChildren list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Attendance</th>
            <th>Date</th>
            <th>Lastupdate</th>
            <th>Datecreate</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('attendancechildren_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.attendance }}</td>
            <td>{% if entity.date %}{{ entity.date|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>{% if entity.lastUpdate %}{{ entity.lastUpdate|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>{% if entity.dateCreate %}{{ entity.dateCreate|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('attendancechildren_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('attendancechildren_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('attendancechildren_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
